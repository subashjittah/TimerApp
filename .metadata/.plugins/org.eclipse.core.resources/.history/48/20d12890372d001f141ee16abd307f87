<xsd:schema xmlns:tns="http://simple.schema/simple" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://simple.schema/simple">
  <xsd:element name="primitive" type="xsd:string"/>

    <xsd:element name="Country" type="xsd:string"></xsd:element>

    <xsd:element name="Sample">
    	<xsd:complexType>
    		<xsd:sequence>
    			<xsd:element ref="tns:Country"></xsd:element>
    		</xsd:sequence>
    	</xsd:complexType>
    </xsd:element>

    <xsd:complexType name="Sample">
    	<xsd:sequence>
    		<xsd:element name="Country" type="xsd:string"></xsd:element>
    	</xsd:sequence>
    </xsd:complexType>
</xsd:schema>
